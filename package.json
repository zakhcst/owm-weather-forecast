{
  "name": "angular-webpack-workflow",
  "version": "1.1.0",
  "description": "A workflow for AngularJS with Webpack",
  "scripts": {
    "build": "rimraf dist && rimraf src/firebase/dist && webpack --bail --progress --profile",
    "server": "webpack-dev-server --history-api-fallback --inline --progress --hot",
    "test": "karma start",
    "test-watch": "karma start --auto-watch --no-single-run",
    "start": "npm run server"
  },
